{
  "name": "angular-io-example",
  "version": "1.0.0",
  "private": true,
  "description": "Example project from an angular.io guide.",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --open --configuration",
    "test": "ng test",
    "lint": "tslint ./src/**/*.ts -t verbose",
    "e2e": "ng e2e",
    "build:ssr": "npm run build:client-and-server-bundles && npm run webpack:server",
    "serve:ssr": "node dist/server.js",
    "build:client-and-server-bundles": "npm run build-i18n && ng run angular.io-example:server",
    "webpack:server": "webpack --config webpack.server.config.js --progress --colors",
    "compodoc": "npx compodoc -p ./tsconfig.json -s",
    "build:prod": "ng build --prod --configuration prod",
    "build:preprod": "ng build --prod --configuration preprod",
    "build:buildtest": "ng build --prod --configuration buildtest",
    "build": "ng build --prod --configuration prod",
    "build-dev": "ng build --prod --configuration dev",
    "build-i18n": "npm run build-i18n:en && npm run build-i18n:ar"
  },
